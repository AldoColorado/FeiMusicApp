<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.feimusic">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FeiMusicApp">
        <activity android:name=".Reproductor"></activity>
        <activity android:name=".MenuPrincipalConsumidor" />
        <activity android:name=".SubirAlbum" />
        <activity android:name=".SubirCancion" />
        <activity android:name=".MenuPrincipalArtista" />
        <activity android:name=".ArtistaRegister" />
        <activity android:name=".ConsumidorRegister" />
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>